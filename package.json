{
  "name": "ts_aws_template",
  "version": "1.0.0",
  "description": "Typescript aws lambda template",
  "main": "app.js",
  "repository": "https://github.com/awslabs/aws-sam-cli/tree/develop/samcli/local/init/templates/cookiecutter-aws-sam-hello-nodejs",
  "author": "TendTo",
  "license": "MIT",
  "scripts": {
    "build": "tsc && cp \"package.json\" \"dist/package.json\" && cp \"package-lock.json\" \"dist/package-lock.json\"",
    "postbuild": "sam build",
    "watch": "tsc -w",
    "test": "jest",
    "invoke": "sam local invoke",
    "invoke:env": "sam local invoke --container-env-vars env.json --env-vars env.json",
    "clean": "rm -r ./.aws-sam"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.85",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "jest": "^27.4.3",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typegram": "^3.5.1",
    "typescript": "^4.5.2"
  }
}
