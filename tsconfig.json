{
    "compilerOptions": {
        "sourceMap": false,
        "esModuleInterop": true,
        "allowJs": true,
        "checkJs": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "lib": [
            "es2020"
        ],
        "target": "es2020",
        "outDir": "./dist",
        "rootDir": "./src"
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.js"
    ],
    "exclude": [
        "src/temp.ts"
    ]
}